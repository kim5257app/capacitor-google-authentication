{
  "name": "@kim5257/capacitor-google-authentication",
  "version": "0.0.12",
  "description": "Google Authentication Capacitor Plugin",
  "main": "dist/index.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/index.js",
  "type": "module",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "Kim5257CapacitorGoogleAuthentication.podspec"
  ],
  "author": "Jaegu Kim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kim5257app/capacitor-google-authentication.git.git"
  },
  "bugs": {
    "url": "https://github.com/kim5257app/capacitor-google-authentication.git/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api GoogleAuthenticationPlugin --output-readme README.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "firebase": "^9.17.1"
  },
  "devDependencies": {
    "@capacitor/android": "^5.4.0",
    "@capacitor/core": "^5.4.0",
    "@capacitor/docgen": "^0.2.1",
    "@capacitor/ios": "^5.4.0",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^7.32.0",
    "prettier": "~3.0.3",
    "prettier-plugin-java": "~2.3.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.2",
    "swiftlint": "^1.0.2",
    "typescript": "~5.2.2"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.4.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
